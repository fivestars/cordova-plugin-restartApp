<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
    id="com.fivestars.mtab.plugin.restartApp"
    version="0.0.1">
    <name>RestartApp Plugin</name>
    <description>Kill Process and Restart activity</description>
    <license></license>
    <keywords></keywords>

    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="RestartAppPlugin">
                <param name="android-package" value="com.fivestars.mtab.plugin.RestartAppPlugin"/>
            </feature>
        </config-file>
        <source-file src="src/android/com/fivestars/mtab/plugin/RestartAppPlugin.java" target-dir="src/com/fivestars/mtab/plugin" />
    </platform>
</plugin>